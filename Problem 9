#include <iostream>

using namespace std;

int main()
{
    int a=0,b=0,c=0,x=0,y=1;
    bool result=false;

    while(!result)
    {
        a++;
        for(b=1;b<500;b++)
        {
            c=1000-a-b;
            if(a*a+b*b==c*c)
            {
                result=true;
                break;
            }
        }


    }

    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    cout<<a*b*c;

    return 0;
}
